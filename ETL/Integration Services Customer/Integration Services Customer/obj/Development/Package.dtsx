                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH DateFirstPurchase AS (
    SELECT 
        CustomerID,
        MIN(OrderDate) AS DateFirstPurchase 
    FROM 
        Sales.SalesOrderHeader
    GROUP BY 
        CustomerID
)
SELECT DISTINCT
    c.CustomerID AS CustomerKey,
    p.FirstName,
    p.MiddleName,
    p.LastName,
    p.NameStyle,
    e.EmailAddress,
    ph.PhoneNumber AS Phone,
    adr.AddressLine1,
    adr.AddressLine2,
    adr.City,
    adr.StateProvinceID,
    adr.PostalCode,
    lo.DateFirstPurchase,
    c.ModifiedDate
FROM 
    Sales.Customer c
JOIN 
    Person.Person p ON c.PersonID = p.BusinessEntityID
LEFT JOIN 
    Person.EmailAddress e ON p.BusinessEntityID = e.BusinessEntityID
LEFT JOIN 
    Person.PersonPhone ph ON p.BusinessEntityID = ph.BusinessEntityID
LEFT JOIN 
    DateFirstPurchase lo ON c.CustomerID = lo.CustomerID

LEFT JOIN 
    Sales.Store st ON c.StoreID = st.BusinessEntityID
LEFT JOIN
	Person.BusinessEntityAddress be ON be.BusinessEntityID = p.BusinessEntityID
LEFT JOIN 
	Person.Address adr ON adr.AddressID = be.AddressID

WHERE 
    c.PersonID IS NOT NULL AND be.AddressTypeID &lt;&gt; 5;</property>
